{
	"name": "nella/forms-datetime",
	"description": "Date/DateTime control for Nette Forms.",
	"homepage": "http://doc.nellafw.org/forms/datetime",
	"type": "library",
	"keywords": ["nette", "forms", "control", "datetime", "date", "input"],
	"license": "MIT",
	"authors": [
	{
		"name": "Patrik Votoček",
		"email": "patrik@votocek.cz",
		"homepage": "http://patrik.votocek.cz",
		"role": "Developer"
	}
	],
	"support": {
		"issues": "https://github.com/nella/forms-datetime/issues",
		"source": "https://github.com/nella/forms-datetime"
	},
	"autoload": {
		"psr-4": {
			"Nella\\Forms\\": "src/Nella/Forms"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/Nella"]
	},
	"require": {
		"php": ">=5.6.0",
		"nette/forms": "~2.3,<2.5.0",
		"nette/di": "~2.3,<2.5.0"
	},
	"require-dev": {
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"nette/tester": "1.7.1",
		"nette/bootstrap": "~2.3,<2.5.0",
		"nella/coding-standard": "4.2.1"
	},
	"suggest": {
		"latte/latte": "For manual rendering support"
	},
	"archive": {
		"exclude": [
			".coveralls.yml",
			".gitattributes",
			".gitignore",
			"/.travis.yml",
			"/.travis.composer.php",
			"/build",
			"/tests"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
